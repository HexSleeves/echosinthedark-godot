config_version=5

[application]

config/name="Echos in the Dark"
run/main_scene="res://scene/main/main_screen.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
run/low_processor_mode=true
config/icon="res://resource/hammer_wrench.png"

[autoload]

EndGame="*res://autoload/end_game.gd"
TransferData="*res://autoload/transfer_data.gd"
